<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="fix_recordDAO">

	<select id="selectAll" resultMap="rsMap">
      SELECT * FROM Fix_record
   </select>
   

      <resultMap type="fix_recordDTO" id="rsMap">
      <result property="a_pro_num" column="n_pro_register" />
      <result property="email" column="n_pro_num" />
      <result property="name" column="n_pro_name" />
      <result property="f_time" column="n_pro_price" />
      <result property="f_price" column="n_pro_amount" />
   </resultMap>

</mapper>
